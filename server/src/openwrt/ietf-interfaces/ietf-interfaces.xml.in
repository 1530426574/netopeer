<device>
	<name>ietf-interfaces</name>
	<data-models>
		<model>
			<path>@sysconfdir@/netopeer/ietf-interfaces/iana-if-type.yin</path>
		</model>
		<model-main>
			<path>@sysconfdir@/netopeer/ietf-interfaces/ietf-interfaces.yin</path>
			<transapi>@sysconfdir@/netopeer/ietf-interfaces/ietf-interfaces.so</transapi>
		</model-main>
		<model>
			<path>@sysconfdir@/netopeer/ietf-interfaces/ietf-ip.yin</path>
			<feature>ipv4-non-contiguous-netmasks</feature>
			<feature>ipv6-privacy-autoconf</feature>
		</model>
		<model>
			<path>@sysconfdir@/netopeer/ietf-interfaces/cesnet-dhcp.yin</path>
		</model>
		<model>
			<path>@sysconfdir@/netopeer/ietf-interfaces/cesnet-wireless.yin</path>
		</model>
	</data-models>
	<repo>
		<type>file</type>
		<path>@sysconfdir@/netopeer/ietf-interfaces/datastore.xml</path>
	</repo>
</device>
